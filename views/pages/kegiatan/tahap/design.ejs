
                        <!-- Konten Design -->
                        <div class="hidden p-6 bg-primary-30 text-medium text-fontdark dark:text-gray-400 dark:bg-gray-800 rounded-lg w-full" id="design" role="tabpanel" aria-labelledby="design-tab">
                            <h3 class="text-lg font-bold text-gray-900 dark:text-white mb-4">Daftar Sub Tahap Design</h3>
                            
                            <div class="mb-6">
                                <p class="text-sm font-medium text-gray-700 mb-2 dark:text-gray-200">Progress Tahap Design:</p>
                                <div class="w-full bg-gray-200 rounded-full dark:bg-gray-700">
                                    <div class="bg-blue-600 text-xs font-medium text-blue-100 text-center p-0.5 leading-none rounded-full" style="width: <%= kegiatan.statusTahap.tahap2Percentage %>%"><%= kegiatan.statusTahap.tahap2Percentage %>%</div>
                                </div>
                            </div>
                            
                            <!-- Table Sub Tahap -->
                            <div class="relative overflow-x-auto shadow-md sm:rounded-lg">
                                <table class="w-full text-sm text-left rtl:text-right text-gray-500 dark:text-gray-400">
                                    <thead class="text-xs text-gray-700 uppercase bg-gray-50 dark:bg-gray-700 dark:text-gray-400">
                                        <tr>
                                            <th scope="col" class="px-6 py-3">
                                                Sub Tahap
                                            </th>
                                            <th scope="col" class="px-6 py-3">
                                                Tanggal Rencana
                                            </th>
                                            <th scope="col" class="px-6 py-3">
                                                Tanggal Realisasi
                                            </th>
                                            <th scope="col" class="px-6 py-3">
                                                Keterangan
                                            </th>
                                            <th scope="col" class="px-6 py-3">
                                                Action
                                            </th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% const subtahapLabels = ['Penyusunan kuesioner', 'Perancangan metodologi survei', 'Penentuan kerangka sampel', 'Pemilihan teknik pengumpulan data']; %>
                                        <% const tahap2 = kegiatan.statusTahap?.tahap2 || {}; %>
                                        <% const subtahapKeys = Object.keys(tahap2).filter(key => !key.includes('tanggal')); %>

                                        <% subtahapKeys.forEach((key, index) => { 
                                            if (index === 0 || index === subtahapKeys.length - 1) return;

                                            const current = tahap2[key];
                                            const prev = index === 1 ? true : tahap2[subtahapKeys[index - 1]];

                                            let status = 'Belum Dimulai';
                                            let color = 'text-gray-500';

                                            if (current === true) {
                                                status = 'Selesai';
                                                color = 'text-green-600';
                                            } else if (prev === true && current === false) {
                                                status = 'Dalam Progress';
                                                color = 'text-yellow-500';
                                            } else if (prev === false || prev === null || prev === undefined) {
                                                status = 'Belum Dimulai';
                                                color = 'text-gray-500';
                                            }

                                            const label = subtahapLabels[index - 1] || `Subtahap ${index + 1}`;

                                            const tanggalKeySuffix = key.replace('subtahap_', '');
                                            const tanggalPerencanaan = tahap2[`subtahap_${tanggalKeySuffix}_tanggal_perencanaan`] || '-';
                                            const tanggalRealisasi = tahap2[`subtahap_${tanggalKeySuffix}_tanggal_realisasi`] || '-';
                                        %>
                                        <tr class="bg-white border-b dark:bg-gray-800 dark:border-gray-700 border-gray-200">
                                            <th scope="row" class="px-6 py-4 font-medium text-gray-900 whitespace-nowrap dark:text-white">
                                                <%= label %>
                                            </th>
                                            <td class="px-6 py-4">
                                                <%= tanggalPerencanaan %>
                                            </td>
                                            <td class="px-6 py-4">
                                                <%= tanggalRealisasi %>
                                            </td>
                                            <td class="px-6 py-4">
                                                <span class="<%= color %> font-semibold"><%= status %></span>
                                            </td>
                                            <td class="px-6 py-4">
                                                <button
                                                    type="button"
                                                    class="text-white bg-blue-600 hover:bg-blue-700 focus:ring-4 focus:outline-none focus:ring-blue-300 font-medium rounded-lg text-sm px-5 py-2.5 dark:bg-blue-500 dark:hover:bg-green-600 dark:focus:ring-blue-700"
                                                    data-modal-target="popup-modal"
                                                    data-modal-toggle="popup-modal"
                                                    data-url="/operator/tahap/<%= kegiatan.id %>/2/<%= index %>">
                                                    Tandai Selesai
                                                </button>
                                            </td>
                                        </tr>
                                        <% }) %>
                                    </tbody>
                                </table>
                            </div>

                            <!-- Tombol Input di FASIH -->
                            <div class="mt-6">
                                <a href="#" class="inline-flex items-center px-5 py-2.5 text-sm font-medium text-center text-white bg-blue-700 rounded-lg hover:bg-blue-800 focus:ring-4 focus:outline-none focus:ring-blue-300 dark:bg-blue-600 dark:hover:bg-blue-700 dark:focus:ring-blue-800">
                                    Input di FASIH
                                    <svg class="w-4 h-4 ml-2" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 14 10">
                                        <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M1 5h12m0 0L9 1m4 4L9 9"/>
                                    </svg>
                                </a>
                                
                                <button id="complete-design" class="ml-4 text-white bg-green-700 hover:bg-green-800 focus:ring-4 focus:outline-none focus:ring-green-300 font-medium rounded-lg text-sm px-5 py-2.5 text-center inline-flex items-center dark:bg-green-600 dark:hover:bg-green-700 dark:focus:ring-green-800">
                                    Selesaikan Tahap Design
                                    <svg class="w-4 h-4 ml-2" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 16 12">
                                        <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M1 5.917 5.724 10.5 15 1.5"/>
                                    </svg>
                                </button>
                            </div>
                        </div>
            