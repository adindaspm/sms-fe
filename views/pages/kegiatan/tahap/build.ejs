
                        <!-- Konten Build -->
                        <div class="p-6 bg-primary-30 text-medium text-fontdark dark:text-gray-400 dark:bg-gray-800 rounded-lg w-full" id="build" role="tabpanel" aria-labelledby="build-tab">
                            <h3 class="text-lg font-bold text-gray-900 dark:text-white mb-4">Daftar Sub Tahap Build</h3>
                            
                            <div class="mb-6">
                                <p class="text-sm font-medium text-gray-700 mb-2 dark:text-gray-200">Progress Tahap Build:</p>
                                <div class="w-full bg-gray-200 rounded-full dark:bg-gray-700">
                                    <div class="bg-blue-600 text-xs font-medium text-blue-100 text-center p-0.5 leading-none rounded-full" style="width: <%= kegiatan.statusTahap.tahap3Percentage %>%"><%= kegiatan.statusTahap.tahap3Percentage %>%</div>
                                </div>
                            </div>
                            
                            <!-- Table Sub Tahap -->
                            <div class="relative overflow-x-auto shadow-md sm:rounded-lg">
                                <table class="w-full text-sm text-left rtl:text-right text-gray-500 dark:text-gray-400">
                                    <thead class="text-xs text-gray-700 uppercase bg-gray-50 dark:bg-gray-700 dark:text-gray-400">
                                        <tr>
                                            <th scope="col" class="px-6 py-3">
                                                Sub Tahap
                                            </th>
                                            <th scope="col" class="px-6 py-3">
                                                Tanggal Rencana
                                            </th>
                                            <th scope="col" class="px-6 py-3">
                                                Tanggal Realisasi
                                            </th>
                                            <th scope="col" class="px-6 py-3">
                                                Keterangan
                                            </th>
                                            <th scope="col" class="px-6 py-3">
                                                Action
                                            </th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% const subtahapLabels = ['Menggunakan kembali atau membangun kuesioner', 'Menggunakan kembali atau membangun komponen proses dan analisis', 'Menggunakan kembali atau membangun komponen diseminasi', 'Konfigurasi alur kerja', 'Uji sistem produksi', 'Uji proses bisnis statistik', 'Finalisasi sistem produksi']; %>
                                        <% const tahap3 = kegiatan.statusTahap?.tahap3 || {}; %>
                                        <% const subtahapKeys = Object.keys(tahap3).filter(key => !key.includes('tanggal')); %>

                                        <% subtahapKeys.forEach((key, index) => { 
                                            if (index === 0 || index === subtahapKeys.length - 1) return;

                                            const current = tahap3[key];
                                            const prev = index === 1 ? true : tahap3[subtahapKeys[index - 1]];

                                            let status = 'Belum Dimulai';
                                            let color = 'text-gray-500';

                                            if (current === true) {
                                                status = 'Selesai';
                                                color = 'text-green-600';
                                            } else if (prev === true && current === false) {
                                                status = 'Dalam Progress';
                                                color = 'text-yellow-500';
                                            } else if (prev === false || prev === null || prev === undefined) {
                                                status = 'Belum Dimulai';
                                                color = 'text-gray-500';
                                            }

                                            const label = subtahapLabels[index - 1] || `Subtahap ${index + 1}`;

                                            const tanggalKeySuffix = key.replace('subtahap_', '');
                                            const tanggalPerencanaan = tahap3[`subtahap_${tanggalKeySuffix}_tanggal_perencanaan`] || '-';
                                            const tanggalRealisasi = tahap3[`subtahap_${tanggalKeySuffix}_tanggal_realisasi`] || '-';
                                        %>
                                        <tr class="bg-white border-b dark:bg-gray-800 dark:border-gray-700 border-gray-200">
                                            <th scope="row" class="px-6 py-4 font-medium text-gray-900 whitespace-nowrap dark:text-white">
                                                <%= label %>
                                            </th>
                                            <td class="px-6 py-4">
                                                <%= tanggalPerencanaan %>
                                            </td>
                                            <td class="px-6 py-4">
                                                <%= tanggalRealisasi %>
                                            </td>
                                            <td class="px-6 py-4">
                                                <span class="<%= color %> font-semibold"><%= status %></span>
                                            </td>
                                            <td class="px-6 py-4 flex items-center align-middle">
                                                <i class="fa-solid fa-calendar-check text-lg p-1
                                                            text-green-600 hover:text-green-700
                                                            <%= status == 'Selesai' ? 'hidden' : '' %>"
                                                    data-tooltip-target="tooltip-tandaiSelesai-<%= kegiatan.id %>-3-<%= index %>"
                                                    data-modal-target="popup-modal"
                                                    data-modal-toggle="popup-modal"
                                                    data-url="/surveys/tahap/<%= kegiatan.id %>/3/<%= index %>">
                                                </i>
                                                <i class="fa-solid fa-calendar-xmark text-lg p-1
                                                            text-blue-600 hover:text-blue-700
                                                            <%= status == 'Selesai' ? '' : 'hidden' %>"
                                                    data-tooltip-target="tooltip-ubahRealisasi-<%= kegiatan.id %>-3-<%= index %>"
                                                    data-modal-target="edit-realisasi"
                                                    data-modal-toggle="edit-realisasi"
                                                    data-url="/surveys/tahap/<%= kegiatan.id %>/3/<%= index %>/realisasi">
                                                </i>
                                                <div id="tooltip-tandaiSelesai-<%= kegiatan.id %>-3-<%= index %>" role="tooltip" class="absolute z-10 invisible inline-block px-3 py-2 text-sm font-medium text-white transition-opacity duration-300 bg-gray-900 rounded-lg shadow-xs opacity-0 tooltip dark:bg-gray-700">
                                                    Tandai Selesai
                                                    <div class="tooltip-arrow" data-popper-arrow></div>
                                                </div>
                                                <div id="tooltip-ubahRealisasi-<%= kegiatan.id %>-3-<%= index %>" role="tooltip" class="absolute z-10 invisible inline-block px-3 py-2 text-sm font-medium text-white transition-opacity duration-300 bg-gray-900 rounded-lg shadow-xs opacity-0 tooltip dark:bg-gray-700">
                                                    Ubah Tanggal Realisasi
                                                    <div class="tooltip-arrow" data-popper-arrow></div>
                                                </div>
                                            </td>
                                        </tr>
                                        <% }) %>
                                    </tbody>
                                </table>
                            </div>

                            <!-- Tombol Input di FASIH -->
                            <div class="mt-6">
                                <button class="text-white bg-green-700 hover:bg-green-800 focus:ring-4 focus:outline-none focus:ring-green-300 font-medium rounded-lg text-sm px-5 py-2.5 text-center inline-flex items-center dark:bg-green-600 dark:hover:bg-green-700 dark:focus:ring-green-800"
                                    data-modal-target="popup-modal"
                                    data-modal-toggle="popup-modal"
                                    data-url="/surveys/completeTahap/<%= kegiatan.id %>/3">
                                    Selesaikan Tahap Build
                                    <svg class="w-4 h-4 ml-2" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 16 12">
                                        <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M1 5.917 5.724 10.5 15 1.5"/>
                                    </svg>
                                </button>
                            </div>
                        </div>
            